            <TableColumn fx:id="appointmentNoCol" prefWidth="125.0" text="Appointment No." />
            <TableColumn fx:id="doctorNameCol" prefWidth="124.0" text="Doctor" />
            <TableColumn fx:id="patientNameCol" prefWidth="114.0" text="Patient" />
            <TableColumn fx:id="roomCol" prefWidth="107.0" text="Room" />
            <TableColumn fx:id="dateCol" prefWidth="110.0" text="Date" />
