<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #E3F2FD;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.hms.Controller.ViewAppointmentController">
   <children>
      <TableView fx:id="appointmentTable" layoutX="110.0" layoutY="113.0" prefHeight="256.0" prefWidth="581.0">
